<div class="container animated fadeIn" style="padding-top:20px;">
	<div class="row" style="text-align:center">
		<h1><%= @post.name %></h1>
		<%= image_tag("placeholder.png", :style=>'height:300px;') %>
		<br>
		<%  if @post.liked?(current_user) %>
		    <%= link_to 'Unlike', unlike_post_path(@post), method: 'post'%>
		<% else %>
		    <%= link_to 'Like', like_post_path(@post), method: 'post'%>
		<% end %>
	</div>
	<div class="row" style="padding-top:40px;">
		<div class="col-md-3 col-pic">
			<%= image_tag("youtube.png", :class=>'pic-url-full') %>

		</div>
		<div class="col-md-3 col-pic">
	        <%= image_tag("reddit.png", :class=>'pic-url-full') %>
		</div>
		<div class="col-md-3 col-pic">
	        <%= image_tag("imgur.png", :class=>'pic-url-full') %>

		</div>
		<div class="col-md-3 col-pic">
	        <%= image_tag("8tracks.svg", :class=>'pic-url-full') %>
		</div>
	</div>
	<div class="row" style="text-align:center; margin-top:40px;">
		<!-- For each non-featured-medium url -->
		<div class="col-md-3 col-md-offset-3">
			<h2>Other Link 1</h2>
		</div>
		<div class="col-md-3">
			<h2>Other Link 1</h2>
		</div>
	</div>
</div>