<div class="container">
  <div class="col-lg-8 col-lg-offset-2" style="text-align:center; margin-top:40px;">
    <div class="jumbotron jumbotron-background">
      <h1>Incorrect login</h1>
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

      <div class="link-box">
        <div class="input-group">
          <span class="input-group-addon">Email</span>
          <%= f.text_field :login, :class=>'form-control', :placeholder=>'example@example.com'%>
        </div>
        <div class="input-group">
          <span class="input-group-addon">Password</span>
          <%= f.password_field :password, :class=>'form-control', :placeholder=>'Password'%>
        </div>
      </div>
      <div class="actions">
        <%= f.submit "Log in", :class=>'btn btn-default' %>
      </div>
    <% end %>
    <div class="row" style="margin-top:20px;">
      <%= render "devise/shared/links" %>
    </div>
    </div>
  </div>
</div>

